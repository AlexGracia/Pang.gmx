            <string>// we move at constant velocity
y -= hookSpeed;

// Collision destructible platform
if (place_meeting(x, y, objDestructiblePatform)){
    with (objDestructiblePatform)
        instance_destroy();
    instance_destroy();
    exit;
}

// if we collide with anything solid delete ourselves
if (!place_free(x, y))
{
    instance_destroy();
}

// do a raycast to check collisions against balls
ballId = collision_line(x, iniY, x, y, objBall, false, true);
if (ballId != noone)
{
    // we hit a ball
    with (ballId)
    {
        audio_play_sound(soundPomp, 0, 0);
        instance_destroy();
    }
    
    // destroy ourselves
    instance_destroy();
}

